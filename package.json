  "main": "src/backend/index.js",
  "scripts": {
    "server:build": "npx tsc --p src/server/tsconfig.json",
    "server:build:watch": "npx tsc --watch --p src/server/tsconfig.json",
    "server:run": "node dist/server/index.js",
    "server:run:watch": "npx nodemon -w dist/server -x 'npm run server:run'",
    "server:dev": "run-p server:build:watch server:run:watch",
    "client:build": "npx webpack --config src/client/webpack.config.js",
    "client:build:watch": "npx webpack --watch --config src/client/webpack.config.js",
    "client:dev": "npx webpack-dev-server --config src/client/webpack.config.js"
  },
