{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "npx ts-node src/index.ts",
    "migrate": ". ./.env && DATABASE_URL=postgres://${SQL_USER}:${SQL_PASSWORD}@${SQL_HOST}:${SQL_PORT}/${SQL_DATABASE} npx node-pg-migrate",
    "start": "npx tsc && npm run migrate up && node dist/index.js",
    "postinstall": "[[ -f ../.env ]] && cp ../.env .env || true"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/node": "^14.0.22",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongodb": "^3.5.9",
    "node-pg-migrate": "^5.2.0",
    "pg": "^8.3.0",
    "redis": "^3.0.2",
    "tslib": "^2.0.0"
  }
}
